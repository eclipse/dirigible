<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>org.eclipse.dirigible</groupId>
		<artifactId>dirigible-groups-parent</artifactId>
		<version>6.2.11</version>
		<relativePath>../pom.xml</relativePath>
	</parent>

	<name>CMS</name>
	<artifactId>dirigible-cms</artifactId>
    <packaging>jar</packaging>

    <build>
        <finalName>${project.artifactId}</finalName>
        <plugins>
            <plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>${maven.compiler.plugin.version}</version>
				<configuration>
					<source>${maven.compiler.source}</source>
					<target>${maven.compiler.target}</target>
					<debug>true</debug>
					<debuglevel>lines,vars,source</debuglevel>
				</configuration>
			</plugin>
        </plugins>
    </build>

    <dependencies>
        <dependency>
            <groupId>org.eclipse.dirigible</groupId>
            <artifactId>dirigible-cms-api</artifactId>
            <version>6.2.11</version>
        </dependency>
        <dependency>
            <groupId>org.eclipse.dirigible</groupId>
            <artifactId>dirigible-cms-internal</artifactId>
            <version>6.2.11</version>
        </dependency>
        <dependency>
            <groupId>org.eclipse.dirigible</groupId>
            <artifactId>dirigible-cms-managed</artifactId>
            <version>6.2.11</version>
        </dependency>

        <dependency>
            <groupId>org.eclipse.dirigible</groupId>
            <artifactId>dirigible-api-facade-cms</artifactId>
            <version>6.2.11</version>
        </dependency>
        <dependency>
            <groupId>org.eclipse.dirigible</groupId>
            <artifactId>dirigible-api-facade-documents</artifactId>
            <version>6.2.11</version>
        </dependency>
        <dependency>
            <groupId>org.eclipse.dirigible</groupId>
            <artifactId>dirigible-api-javascript-cms</artifactId>
            <version>6.2.11</version>
        </dependency>
        <dependency>
            <groupId>org.eclipse.dirigible</groupId>
            <artifactId>dirigible-api-javascript-documents</artifactId>
            <version>6.2.11</version>
        </dependency>
        <dependency>
            <groupId>org.eclipse.dirigible</groupId>
            <artifactId>dirigible-ide-ui-core</artifactId>
            <version>6.2.11</version>
        </dependency>
        <dependency>
		 	<groupId>org.eclipse.dirigible</groupId>
		 	<artifactId>dirigible-ide-ui-documents</artifactId>
		 	<version>6.2.11</version>
		</dependency>
		<dependency>
			<groupId>org.eclipse.dirigible</groupId>
			<artifactId>dirigible-ide-ui-documents-ext-content-type-ms</artifactId>
			<version>6.2.11</version>
	    </dependency>
	    <dependency>
			<groupId>org.eclipse.dirigible</groupId>
			<artifactId>dirigible-ide-ui-csv</artifactId>
			<version>6.2.11</version>
		</dependency>
        <dependency>
			<groupId>org.eclipse.dirigible</groupId>
			<artifactId>dirigible-ide-ui-csvim</artifactId>
			<version>6.2.11</version>
		</dependency>
		<dependency>
			<groupId>org.eclipse.dirigible</groupId>
			<artifactId>dirigible-cms-csvim</artifactId>
			<version>6.2.11</version>
		</dependency>
    </dependencies>

    <properties>
        <license.header.location>../../licensing-header.txt</license.header.location>
    </properties>

</project>