		<fd-panel expanded="false">
			<fd-panel-header>
				<fd-panel-expand></fd-panel-expand>
				<h4 fd-panel-title>Bar</h4>
			</fd-panel-header>
			<fd-panel-content aria-label="Bar Panel Content">
				<h4>Bar</h4>
				<fd-bar>
					<fd-bar-left>
						<fd-bar-element>
							<fd-button glyph="sap-icon--navigation-left-arrow" compact="true" aria-label="button"
								dg-type="transparent">
							</fd-button>
						</fd-bar-element>
						<fd-bar-element>
							<fd-button glyph="sap-icon--home" aria-label="button" compact="true" dg-type="transparent">
							</fd-button>
						</fd-bar-element>
						<fd-bar-element>
							<fd-button glyph="sap-icon--account" aria-label="button" compact="true"
								dg-type="transparent"></fd-button>
						</fd-bar-element>
					</fd-bar-left>
					<fd-bar-middle>
						<fd-bar-element>
							<fd-segmented-button aria-label="Test group">
								<fd-button glyph="sap-icon--email" compact="true" aria-label="button" dg-toggled="true">
								</fd-button>
								<fd-button glyph="sap-icon--iphone" compact="true" aria-label="button"></fd-button>
								<fd-button glyph="sap-icon--notification-2" compact="true" aria-label="button">
