		<fd-panel expanded="false">
			<fd-panel-header>
				<fd-panel-expand></fd-panel-expand>
				<h4 fd-panel-title>ComboBox</h4>
			</fd-panel-header>
			<fd-panel-content aria-label="ComboBox Panel Content" style="height:34.375rem">
				<h4>ComboBox Input</h4>
				<div style="display:flex; justify-content: space-around;align-items: center;">
					<fd-combobox-input compact="true" dropdown-items="comboboxItems" dg-placeholder="Search fruit ..."
						dg-aria-label="fruits">
					</fd-combobox-input>

					<fd-combobox-input dropdown-items="comboboxItems" dg-disabled="true" selected-value="1"
						dg-placeholder="Search fruit ..." dg-aria-label="fruits">
					</fd-combobox-input>

					<fd-combobox-input dropdown-items="comboboxItems2" ng-model="combobox.selectedModelValue"
						ng-change="combobox.onCBChange()" required dg-placeholder="Search product ..."
						dg-aria-label="products">
					</fd-combobox-input>

					<fd-combobox-input input-id="test" dropdown-items="comboboxItems" dg-placeholder="Search fruit ..."
						state="success" message="Success message" dg-aria-label="fruits"></fd-combobox-input>
				</div>

				<h4>ComboBox Input Dynamic</h4>
				<fd-combobox-input input-id="dynamic" dropdown-items="dynamicItems"
					ng-keyup="onComboInputChange($event)" dg-placeholder="Start typing"
					dg-aria-label="dynamic combobox">
				</fd-combobox-input>

				<h4>Multi Combobox</h4>
				<fd-form-item>
					<fd-combobox-input multi-select="true" ng-model="combobox.selectedModelValues"
						ng-change="combobox.onMCBChange()" dropdown-items="multiComboboxItems"
						dg-placeholder="Search fruit ..." dg-aria-label="fruits">
					</fd-combobox-input>
				</fd-form-item>
			</fd-panel-content>
		</fd-panel>
