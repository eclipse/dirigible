			</fd-panel-header>
			<fd-panel-content aria-label="Wizard Panel Content">
				<fd-wizard dg-size="md" current-step="wizard.currentStep" completed-steps="wizard.completedSteps">
					<header>
						<fd-bar bar-design="header-with-subheader" cozy="true" in-page="true" size="m_l">
							<fd-bar-left>
								<fd-bar-element>
									<h1 fd-title header-size="3">Wizard with steps</h1>
								</fd-bar-element>
							</fd-bar-left>
						</fd-bar>
						<fd-wizard-navigation></fd-wizard-navigation>
					</header>
					<fd-wizard-content dg-size="md" style="min-height: 300px;">
						<fd-wizard-step dg-size="md" dg-label="Step 1" indicator-label="1"
							indicator-glyph="{{ getIndicatorGlyph(1) }}" step-click="gotoStep(step)">
							<div>Step 1 content</div>
							<fd-button dg-label="Revert" dg-type="emphasized" ng-click="revert(0)"></fd-button>
						</fd-wizard-step>
						<fd-wizard-step dg-label="Step 2" indicator-label="2"
							indicator-glyph="{{ getIndicatorGlyph(2) }}" step-click="gotoStep(step)">
							<div>Step 2 content</div>
							<fd-button dg-label="Revert" dg-type="emphasized" ng-click="revert(1)"></fd-button>
						</fd-wizard-step>
						<fd-wizard-step dg-label="Step 3" indicator-label="3"
							indicator-glyph="{{ getIndicatorGlyph(3) }}" step-click="gotoStep(step)">
							<div>Step 3 content</div>
							<fd-button dg-label="Revert" dg-type="emphasized" ng-click="revert(2)"></fd-button>
						</fd-wizard-step>
						<fd-wizard-step dg-label="Step 4" indicator-label="4"
							indicator-glyph="{{ getIndicatorGlyph(4) }}" step-click="gotoStep(step)">
							<div>Step 4 content</div>
						</fd-wizard-step>
						<fd-wizard-summary>
							<div>Summary content</div>
						</fd-wizard-summary>
					</fd-wizard-content>
					<footer>
						<fd-bar bar-design="footer" in-page="true" size="m_l">
							<fd-bar-right>
								<fd-bar-element>
									<fd-button ng-if="!allStepsCompleted()" dg-label="Previous" compact="true"
										ng-click="gotoPreviousStep()">
									</fd-button>
								</fd-bar-element>
								<fd-bar-element>
									<fd-button ng-if="!allStepsCompleted()"
										dg-label="{{ isLastStep() ? 'Finish' : 'Next' }}" dg-type="emphasized"
										compact="true" ng-click="gotoNextStep()"></fd-button>
								</fd-bar-element>
								<fd-bar-element>
									<fd-button ng-if="!allStepsCompleted()" dg-label="Cancel" dg-type="transparent"
										compact="true"></fd-button>
								</fd-bar-element>
							</fd-bar-right>
						</fd-bar>
					</footer>
				</fd-wizard>
