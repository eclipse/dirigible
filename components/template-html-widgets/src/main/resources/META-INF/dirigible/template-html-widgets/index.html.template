						<fd-bar-element>
							<fd-button glyph="sap-icon--navigation-left-arrow" aria-label="button"
								dg-type="transparent">
							</fd-button>
						</fd-bar-element>
						<fd-bar-element>
							<fd-button glyph="sap-icon--home" aria-label="button" dg-type="transparent"></fd-button>
						</fd-bar-element>
						<fd-bar-element>
							<fd-button glyph="sap-icon--account" aria-label="button" dg-type="transparent"></fd-button>
						</fd-bar-element>
					</fd-bar-left>
					<fd-bar-middle>
						<fd-bar-element>
							<fd-segmented-button aria-label="Test group">
								<fd-button glyph="sap-icon--email" aria-label="button" dg-toggled="true"></fd-button>
								<fd-button glyph="sap-icon--iphone" aria-label="button"></fd-button>
								<fd-button glyph="sap-icon--notification-2" aria-label="button"></fd-button>
							</fd-segmented-button>
						</fd-bar-element>
					</fd-bar-middle>
					<fd-bar-right>
						<fd-bar-element>
							<fd-button glyph="sap-icon--grid" aria-label="button" dg-type="transparent">
							</fd-button>
						</fd-bar-element>
					</fd-bar-right>
				</fd-bar>

				<h4>Bar header with subheader and title</h4>
				<fd-bar bar-design="header-with-subheader">
					<fd-bar-left>
						<fd-bar-element is-title="true">
							<h6 fd-title dg-wrap="true" aria-label="title">TEXT</h6>
						</fd-bar-element>
					</fd-bar-left>
					<fd-bar-middle>
						<fd-bar-element>
							<fd-segmented-button aria-label="Test group">
								<fd-button glyph="sap-icon--email" aria-label="button" dg-toggled="true" compact="true">
								</fd-button>
								<fd-button glyph="sap-icon--iphone" aria-label="button" compact="true"></fd-button>
								<fd-button glyph="sap-icon--notification-2" aria-label="button" compact="true">
								</fd-button>
							</fd-segmented-button>
						</fd-bar-element>
					</fd-bar-middle>
					<fd-bar-right>
						<fd-bar-element>
							<fd-button glyph="sap-icon--grid" aria-label="button" dg-type="transparent" compact="true">
							</fd-button>
						</fd-bar-element>
					</fd-bar-right>
				</fd-bar>
				<fd-bar bar-design="subheader">
					<fd-bar-middle>
						<fd-bar-element full-width="true">
							<fd-input type="text" compact="true" in-group="true" value="1234568910 " aria-label="input">
							</fd-input>
						</fd-bar-element>
					</fd-bar-middle>
				</fd-bar>

				<h4>Bar footer</h4>
				<fd-bar bar-design="footer">
					<fd-bar-right>
						<fd-bar-element>
							<fd-button dg-label="Save" dg-type="emphasized" compact="true"></fd-button>
						</fd-bar-element>
						<fd-bar-element>
							<fd-button dg-label="Cancel" dg-type="transparent" compact="true"></fd-button>
						</fd-bar-element>
					</fd-bar-right>
				</fd-bar>
			</fd-panel-content>
		</fd-panel>
		<fd-panel expanded="false">
			<fd-panel-header>
				<fd-panel-expand></fd-panel-expand>
				<h4 fd-panel-title>Busy indicator</h4>
			</fd-panel-header>
			<fd-panel-content aria-label="Busy Indicator Panel Content">
				<h4>Sizes</h4>
				<fd-busy-indicator dg-size="m"></fd-busy-indicator>
				<fd-busy-indicator dg-size="l"></fd-busy-indicator>
				<h4>With text</h4>
				<fd-busy-indicator-extended dg-size="l">Very busy...</fd-busy-indicator-extended>
			</fd-panel-content>
		</fd-panel>
		<fd-panel expanded="false">
			<fd-panel-header>
				<fd-panel-expand></fd-panel-expand>
				<h4 fd-panel-title>Button</h4>
			</fd-panel-header>
			<fd-panel-content aria-label="Button Panel Content">
				<h4>Button styles</h4>
				<fd-button dg-label="Default"></fd-button>
				<fd-button dg-label="Emphasized" dg-type="emphasized"></fd-button>
				<fd-button dg-label="Ghost" dg-type="ghost"></fd-button>
				<fd-button dg-label="Positive" dg-type="positive"></fd-button>
				<fd-button dg-label="Negative" dg-type="negative"></fd-button>
				<fd-button dg-label="Attention" dg-type="attention"></fd-button>
				<fd-button dg-label="Transparent" dg-type="transparent"></fd-button>
				<h4>Button states</h4>
				<fd-button dg-label="Normal"></fd-button>
				<fd-button dg-label="Disabled" state="disabled"></fd-button>
				<fd-button dg-label="Disabled focusable" state="disabled-focusable"
					dg-aria-desc="disabled focusable button"></fd-button>
				<h4>Compact button</h4>
				<fd-button dg-label="Default" compact="true"></fd-button>
				<h4>Toggle button</h4>
				<fd-button dg-label="Toggle" ng-click="toggle()" dg-toggled="btnToggled"></fd-button>
				<h4>Segmented button</h4>
				<fd-segmented-button aria-label="Segmented group">
					<fd-button dg-label="Left" dg-toggled="segmentedModel === 'left' ? true : false"
						ng-click="segmentedClick('left')">
					</fd-button>
					<fd-button dg-label="Middle" dg-toggled="segmentedModel === 'middle' ? true : false"
						ng-click="segmentedClick('middle')">
					</fd-button>
					<fd-button dg-label="Right" dg-toggled="segmentedModel === 'right' ? true : false"
						ng-click="segmentedClick('right')">
					</fd-button>
				</fd-segmented-button>
				<h4>Menu button</h4>
				<fd-button dg-label="Selection menu" is-menu="true" dg-aria-desc="menu button">
				</fd-button>
				<h4>Split menu button</h4>
				<fd-split-button style="margin-bottom: 100px" main-action="{{ splitButtonAction }}" dg-align="right"
					aria-label="button-split" callback="splitButtonClick">
					<fd-menu aria-label="split button items">
						<fd-menu-item title="Item" ng-click="splitItemClick('Item')"></fd-menu-item>
						<fd-menu-item title="Item 2" ng-click="splitItemClick('Item 2')"></fd-menu-item>
						<fd-menu-item title="Item 3" ng-click="splitItemClick('Item 3')"></fd-menu-item>
					</fd-menu>
				</fd-split-button>
				<h4>Icon button</h4>
				<fd-button glyph="sap-icon--cart" aria-label="Add to cart">
				</fd-button>
				<fd-button dg-label="Add to cart" glyph="sap-icon--cart" aria-label="Add to cart">
				</fd-button>
				<h4>Button with badge</h4>
				<fd-button dg-label="Cart" glyph="sap-icon--cart" aria-label="Cart" badge="121">
				</fd-button>
				<fd-button dg-label="Cart" glyph="sap-icon--cart" aria-label="Cart" badge="121" compact="true">
				</fd-button>
			</fd-panel-content>
		</fd-panel>
		<fd-panel expanded="false">
			<fd-panel-header>
				<fd-panel-expand></fd-panel-expand>
				<h4 fd-panel-title>Card</h4>
			</fd-panel-header>
			<fd-panel-content aria-label="Card Panel Content">
				<h4>Card</h4>
				<fd-card card-type="list">
					<fd-badge>New</fd-badge>
					<fd-card-header interactive="false">
						<fd-avatar image="http://picsum.photos/id/1018/400" dg-size="s" circle="true"
							aria-label="avatar">
						</fd-avatar>
						<fd-card-title>Card Title</fd-card-title>
						<fd-card-subtitle>Card Subtitle</fd-card-subtitle>
						<fd-card-status dg-type="counter" status="positive"> 1 of 10</fd-card-status>
					</fd-card-header>
					<fd-card-content>
						<fd-list no-border="true">
							<fd-list-item tabindex="0">
								<fd-list-title>Item 1</fd-list-title>
							</fd-list-item>
							<fd-list-item tabindex="0">
								<fd-list-title>Item 2</fd-list-title>
							</fd-list-item>
							<fd-list-item tabindex="0">
								<fd-list-title>Item 3</fd-list-title>
							</fd-list-item>
						</fd-list>
					</fd-card-content>
				</fd-card>
				<h4>Card with table</h4>
				<fd-card card-type="table">
					<fd-badge>New</fd-badge>
					<fd-card-header>
						</fd-avatar>
						<fd-card-title>Table Card</fd-card-title>
						<fd-card-subtitle>Compact mode</fd-card-subtitle>
						<fd-card-status dg-type="counter" status="positive"> 4 of 10</fd-card-status>
					</fd-card-header>
					<fd-card-content>
						<table fd-table inner-borders="top" outer-borders="none">
							<thead fd-table-header>
								<tr fd-table-row>
									<td fd-table-cell content-type="checkbox">
										<fd-checkbox id="cbid12"></fd-checkbox>
										<fd-checkbox-label empty="true" for="cbid12"></fd-checkbox-label>
									</td>
									<th fd-table-header-cell>Name</th>
									<th fd-table-header-cell>Status</th>
									<th fd-table-header-cell>Price</th>
									<th fd-table-header-cell>Country</th>
									<th fd-table-header-cell></th>
								</tr>
							</thead>
							<tbody fd-table-body>
								<tr fd-table-row hoverable="true" activable="true">
									<td fd-table-cell content-type="checkbox">
										<fd-checkbox id="cbid12_1"></fd-checkbox>
										<fd-checkbox-label empty="true" for="cbid12_1"></fd-checkbox-label>
									</td>
									<td fd-table-cell>Banana</td>
									<td fd-table-cell><span fd-object-status status="positive" text="Available"></span>
									</td>
									<td fd-table-cell>5 EUR</td>
									<td fd-table-cell>India</td>
									<td fd-table-cell fit-content="true">
										<i fd-table-icon navigation="true" glyph="sap-icon--slim-arrow-right"></i>
									</td>
								</tr>
								<tr fd-table-row hoverable="true" activable="true">
									<td fd-table-cell content-type="checkbox">
										<fd-checkbox id="cbid12_2"></fd-checkbox>
										<fd-checkbox-label empty="true" for="cbid12_2"></fd-checkbox-label>
									</td>
									<td fd-table-cell>Pineapple</td>
									<td fd-table-cell><span fd-object-status status="negative"
											text="Out of stock"></span>
									</td>
									<td fd-table-cell>2 EUR</td>
									<td fd-table-cell>Mexico</td>
									<td fd-table-cell fit-content="true">
										<i fd-table-icon navigation="true" glyph="sap-icon--slim-arrow-right"></i>
									</td>
								</tr>
								<tr fd-table-row hoverable="true" activable="true">
									<td fd-table-cell content-type="checkbox">
										<fd-checkbox id="cbid12_3"></fd-checkbox>
										<fd-checkbox-label empty="true" for="cbid12_3"></fd-checkbox-label>
									</td>
									<td fd-table-cell>Orange</td>
									<td fd-table-cell><span fd-object-status status="informative"
											text="Temporary unavailable"></span>
									</td>
									<td fd-table-cell>6 EUR</td>
									<td fd-table-cell>Spain</td>
									<td fd-table-cell fit-content="true">
										<i fd-table-icon navigation="true" glyph="sap-icon--slim-arrow-right"></i>
									</td>
								</tr>
							</tbody>
						</table>
					</fd-card-content>
				</fd-card>
			</fd-panel-content>
		</fd-panel>
